{
	"rules":{
		".read": "auth.uid!=null",
		"exams":{
			"$eid":{
				".write":true
			}
		},
		"versions":{
			"$vid":{
				".write":true,
				".validate":"root.child('exams').child(newData.child('examId').val()).exists()"
			}
		},
		"questions":{
			"$qid":{
				".write":true,
				".validate":"root.child('versions').child(newData.child('versionId').val()).exists()"
			}
		}
	}
}